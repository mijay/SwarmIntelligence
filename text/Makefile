all: dvi

allpdf: pdf

help: .help
	@echo "  all          alias for dvi target" ;\
	 echo "  allpdf       alias for pdf target" ;\

clear: 
	-rm *.aux *.log *.toc *.bbl *.blg *.nlo *.nls *.ilg
